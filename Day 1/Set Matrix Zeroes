#include <bits/stdc++.h> 
void setZeros(vector<vector<int>> &matrix)
{
	vector<int>col;
        vector<int>row;
        for(int i=0;i<matrix.size();i++){
            for(int j=0;j<matrix[i].size();j++){
                if(matrix[i][j] == 0){
                    row.push_back(i);
                    col.push_back(j);
                }
            }
        }
        for(int k:row){
            for(int i=0;i<matrix.size();i++){
            for(int j=0;j<matrix[i].size();j++){
                if(k==i) matrix[i][j]=0;
            }
            }
        }
        for(int k:col){
            for(int i=0;i<matrix.size();i++){
            for(int j=0;j<matrix[i].size();j++){
                if(k==j) matrix[i][j]=0;
            }
            }
        }
}
